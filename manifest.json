{
  "manifest_version": 2,
  "name": "Jonesy timer",
  "version": "1.0",
  "description": "Simple timer extension",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "unlimitedStorage"
  ],
  "browser_action": {},
  "options_page": "src/options/options.html",
  "background": {
    "page": "src/background/background.html"
  },
  "icons": {
    "16": "src/assets/images/clock-16x16.png",
    "48": "src/assets/images/clock-48x48.png",
    "128": "src/assets/images/clock-128x128.png"
  },
  "commands": {
    "toggle-timer": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"
      },
      "description": "Start/Pause timer",
      "global": true
    }
  }
}
