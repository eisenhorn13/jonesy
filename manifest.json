{
  "name": "Jonesy timer",
  "version": "1.0",
  "description": "Simple timer extension for Google Chrome",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
  "browser_action": {},
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "Timer.js"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "assets/images/clock-16x16.png",
    "48": "assets/images/clock-48x48.png",
    "128": "assets/images/clock-128x128.png"
  }
}
