{
  "name": "Jonesy timer",
  "version": "1.0",
  "description": "Simple timer extension for Google Chrome",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications"
  ],
  "browser_action": {},
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "manifest_version": 2,
  "icons": {
    "16": "clock.png",
    "32": "clock.png",
    "48": "clock.png",
    "128": "clock.png"
  }
}
